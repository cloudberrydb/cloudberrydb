-------+----
     1 | 11
(1 row)

-- function in subselect, with correlation
select * from foo2 
where f2 in (select f2 from foost(foo2.fooid) z where z.fooid = foo2.fooid) 
ORDER BY 1,2;
 fooid | f2 
-------+----

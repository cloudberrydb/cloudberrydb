-- start_matchignore
# Mask out timestamp and whoami message for gpfaultinjector
m/^\d+.*gpfaultinjector.*-\[INFO\]:-/
s/^\d+.*gpfaultinjector.*-\[INFO\]:-//
-- end_matchignore

-- start_matchsubs
# entry db matches
m/\s+\(entry db(.*)+\spid=\d+\)/
s/\s+\(entry db(.*)+\spid=\d+\)//

# remove beginning output of gpconfig
m/^\d+.*gpconfig.*-\[INFO\]:-/
s/^\d+.*gpconfig.*-\[INFO\]:-//
-- end_matchsubs

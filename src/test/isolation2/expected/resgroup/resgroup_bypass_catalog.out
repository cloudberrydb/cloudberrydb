BEGIN

-- two slot have all been taken, so this query will be hung up.
3: SET ROLE role_test_catalog;
SET
3&: BEGIN;  <waiting ...>

-- It's a catalog only query, so it will be bypassed.
4: SET ROLE role_test_catalog;
SET

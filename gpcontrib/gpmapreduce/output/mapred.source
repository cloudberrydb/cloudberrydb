CREATE OR REPLACE FUNCTION parse_pg_rank_kv(s text) returns text as $$
i = s.index(",")
rank = s[:i]
neighbors = list(map(int, s[i+1:].strip().strip("()").split(",")))
neighbors.sort()
ns = "(" + ",".join(map(str, neighbors)) + ")"
return ", ".join([rank, ns])
$$ LANGUAGE plpython3u NO SQL;
